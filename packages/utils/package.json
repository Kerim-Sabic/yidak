{
  "name": "@yidak/utils",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./currency": {
      "types": "./dist/currency.d.ts",
      "default": "./dist/currency.js"
    },
    "./date": {
      "types": "./dist/date.d.ts",
      "default": "./dist/date.js"
    },
    "./result": {
      "types": "./dist/result.d.ts",
      "default": "./dist/result.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "lint": "eslint .",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest run",
    "clean": "powershell -NoProfile -Command \"Remove-Item -Recurse -Force dist -ErrorAction SilentlyContinue\""
  },
  "dependencies": {
    "@yidak/types": "workspace:*",
    "neverthrow": "^8.1.1"
  },
  "devDependencies": {
    "@yidak/eslint-config": "workspace:*",
    "@yidak/typescript-config": "workspace:*",
    "eslint": "^9.20.1",
    "vitest": "^2.1.8"
  }
}
